{
  "name": "onelayout-monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "test": "turbo run lint && jest packages/onelayout/",
    "test:watch": "jest packages/onelayout/ --watch",
    "test:staged": "jest packages/onelayout/ --bail --findRelatedTests",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "@types/jest": "^29.5.2",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "prettier": "latest",
    "turbo": "latest",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "npm@8.19.2"
}
